    "import qi\n",
    "import sys\n",
    "\n",
    "class Foo:\n",
    "\n",
    "    def bar(self):\n",
    "        print(\"bar\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    app = qi.Application(sys.argv)\n",
    "\n",
    "    # start the session\n",
    "    app.start()\n",
    "\n",
    "    app.session.registerService(\"foo\", Foo())\n",
    "\n",
    "    app.run()   # will exit when the connection is over"
   ]
  }
